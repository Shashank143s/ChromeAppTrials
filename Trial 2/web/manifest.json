{
  "manifest_version": 2,
  "name": "ToDoListTrial",
  "version": "1",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": ["storage"],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "minimum_chrome_version": "28",
  "sandbox": { "pages": [ "index.html" ] },
  "permissions": [ "<all_urls>", "*://*/*" ]
}
